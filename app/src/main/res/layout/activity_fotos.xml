<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FotosActivity">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/voltarCadastroFotoPerfilId"
            android:layout_width="185dp"
            android:layout_height="185dp"
            app:srcCompat="@drawable/icon_ju" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="0"
        android:layout_row="1"
        app:srcCompat="@drawable/icon_ju_3" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="0"
        android:layout_row="2"
        app:srcCompat="@drawable/icon_ju_2" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="1"
        android:layout_row="0"
        app:srcCompat="@drawable/icon_ju_4" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="1"
        android:layout_row="1"
        app:srcCompat="@drawable/icon_ju_5" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="1"
        android:layout_row="2"
        app:srcCompat="@drawable/icon_ju_6" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="0"
        android:layout_row="3"
        app:srcCompat="@drawable/icon_ju_7" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="185dp"
        android:layout_height="185dp"
        android:layout_column="1"
        android:layout_row="3"
        app:srcCompat="@drawable/icon_ju_8" />

    </GridLayout>

</ScrollView>